{
  "name": "bot-starter",
  "version": "1.0",
  "description": "TRIM Typescript Starter for Slackbots",
  "main": "src/server.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "docker:build": "docker-compose build -t ${npm_package_name}:${npm_package_version} .",
    "start": "node dist/server.js",
    "dev": "nodemon -L",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --coverage --watchAll --detectOpenHandles",
    "build:watch": "onchange 'src/**/*.ts' -- yarn run build",
    "clean": "rm -rf coverage dist",
    "heroku-postbuild": "yarn run build",
    "seeds-up": "ts-node src/db/seeders/seeds-up.ts",
    "seeds-down": "ts-node src/db/seeders/seeds-down.ts"
  },
  "engines": {
    "node": "14.1.0"
  },
  "keywords": [
    "TRIM",
    "Slackbot",
    "Botkit",
    "Typescript"
  ],
  "author": "TRIM - <services@trimagency.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "botbuilder-adapter-slack": "^1.0.10",
    "botbuilder-storage-mongodb": "^0.9.5",
    "botkit": "^4.8.1",
    "botkit-storage-mongo": "^1.0.7",
    "bull": "^3.14.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.13",
    "pg": "^8.2.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.11",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^8.10.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/bull": "^3.12.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.7.16",
    "@types/node": "^13.13.5",
    "@types/redis": "^2.8.20",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.1.0",
    "jest": "^25.5.0",
    "nodemon": "^2.0.3",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.0",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  }
}